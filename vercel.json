{
  "builds": [
    {
      "src": "btre/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python:3.12.2",
        "installCommand": "pip install psycopg2-binary && pip install -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "btre/wsgi.py"
    }
  ]
}
